#%RAML 1.0
title: MsgB
properties:
  txt:
    type: string
    description: Некое текстовое сообщение
    required: true
  createdDt:
    type: datetime
    description: Дата формирования сообщения
    required: true
    format: rfc3339
  currentTemp:
    type: integer
    description: Температура по Цельсию
    required: true
example:
  {
    "txt": "Привет",
    "createdDt": "2020-06-10T10:15:30Z",
    "currentTemp": "28"
  }